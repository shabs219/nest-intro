{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "strictBindCallApply": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {
      "@testApp/keycloak-connect-nest-lib": [
        "libs/keycloak-connect-nest-lib/src"
      ],
      "@testApp/keycloak-connect-nest-lib/*": [
        "libs/keycloak-connect-nest-lib/src/*"
      ],
      "@app/example-library2121": [
        "libs/example-library2121/src"
      ],
      "@app/example-library2121/*": [
        "libs/example-library2121/src/*"
      ]
    }
  }
}